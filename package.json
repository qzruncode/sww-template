{
  "name": "sww-template",
  "version": "1.0.0",
  "description": "sww-cli的应用模板，用于快速生成demo应用",
  "homepage": "https://github.com/qzruncode/sww-template#readme",
  "sideEffects": [
    "*.less",
    "*.css"
  ],
  "scripts": {
    "start": "sww run -m Dev -h sww-template.com -H -url /",
    "build": "sww run -m Pro -url /",
    "preview": "http-server build",
    "reinstall": "rm -rf node_modules && npm install --registry=https://registry.npm.alibaba-inc.com",
    "tsc": "tsc",
    "fix": "npx eslint --fix",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js}": [
      "prettier --write",
      "npm run fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:qzruncode/sww-template.git"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/core-decorators": "^0.20.0",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "cacheweb-webpack-plugin": "^2.0.3",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.9.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "http-server": "^0.12.3",
    "husky": "^7.0.1",
    "lint-staged": "^11.0.1",
    "prettier": "^2.3.2",
    "sww-cli": "^2.0.2"
  },
  "dependencies": {
    "core-js": "^3.18.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
